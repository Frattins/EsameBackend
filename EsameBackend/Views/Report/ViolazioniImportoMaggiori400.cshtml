@model IEnumerable<EsameBackend.Models.Verbale>

<h1>Violazioni con Importo Maggiore di 400€</h1>

<table class="table">
    <thead>
        <tr>
            <th>Cognome</th>
            <th>Nome</th>
            <th>Importo</th>
            <th>Data Violazione</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Anagrafica.Cognome</td>
                <td>@item.Anagrafica.Nome</td>
                <td>@item.Importo</td>
                <td>@item.DataViolazione</td>
            </tr>
        }
    </tbody>
</table>
